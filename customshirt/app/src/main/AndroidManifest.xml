<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.customshirt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logocs"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logocs_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:icon">
        <activity android:name=".AlamatActivity"></activity>
        <activity android:name=".KurirActivity" />
        <activity
            android:name=".PopupDeleteCart"
            android:theme="@style/AppTheme.Customtheme" />
        <activity android:name=".CheckoutActivity" />
        <activity android:name=".EditAlamatActivity" />
        <activity android:name=".PengirimanActivity" />
        <activity android:name=".TestInputDesain" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".SearchActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailItem" />
        <activity android:name=".LupaPasswordActivity" />
        <activity
            android:name=".ButtonNav"
            android:label="Beranda" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/NoActionBar" />
    </application>

</manifest>